package by.epam.grodno.trainig.val.gri.se02;

/**
 * @author Gri
 *
 */
public class Stationery {
	private int price;
	private String type;
	private int weigh;
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public int getWeigh() {
		return weigh;
	}
	public void setWeigh(int weigh) {
		this.weigh = weigh;
	}
	

}
